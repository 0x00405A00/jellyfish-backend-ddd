{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "WebFrontEnd.Services": "Warning",
      "Shared.Infrastructure.Backend.Api": "Debug",
      "Shared.Infrastructure.Backend.SignalR": "Debug"
    }
  },
  "Infrastructure": {
    "Api": {
      "BaseUrl": "http://webapi:5000/api/v1",
      "LoginEndpoint": "/authentification/login",
      "LogoutEndpoint": "/authentification/logout",
      "RegisterEndpoint": "/user/register",
      "ValidateEndpoint": "/",
      "RefreshSessionEndpoint": "/authentification/refresh",
      "ConnectionTestEndpoint": "/user/password/reset/request",
      "PasswordResetEndpoint": "/user/password/reset/",
      "PasswordResetRequestEndpoint": "/",
      "HealthCheckController": "http://webapi:5000/health",
      "HealthCheck": "http://webapi:5000/healthz"
    },
    "SignalR": {
      "SignalRHubClientTransportProtocol": 0,
      "SignalRHubBaseUrl": "http://webapi",
      "SignalRHubEndpoint": "/messengerhub",
      "SignalRHubBaseUrlPort": 5000
    }
  },
  "AllowedHosts": "*"
}
